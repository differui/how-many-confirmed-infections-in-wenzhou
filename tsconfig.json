{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["dom", "es2015"],
    "target": "ES5",
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "downlevelIteration": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.json"]
}
